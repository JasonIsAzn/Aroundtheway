{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "defaultConnection": "Server=localhost;Port=3307;Database=AppDb;User={{DB_USER}};Password={{DB_PASSWORD}};"
  },
  "Supabase": {
    "Url": "{{SUPABASE_URL}}",
    "Key": "{{SUPABASE_KEY}}",
    "BucketName": "{{SUPABASE_BUCKET_NAME}}"
  },
  "AWS": {
    "AccessKey": "{{AWS_ACCESS_KEY}}",
    "SecretKey": "{{AWS_SECRET_KEY}}",
    "ServiceURL": "{{AWS_SERVICE_URL}}",
    "BucketName": "{{AWS_BUCKET_NAME}}",
    "Region": "{{AWS_REGION}}"
  },
  "Stripe": {
    "SecretKey": "{{STRIPE_SECRET_KEY}}"
  }
}
